<h1>Welcome to SoftUni Wiki!</h1>
<section>
  <h2>{{article.title}}</h2>
  <div class="spacer">
    <h3>Edit hostory</h3>
    <ul>
      {{#each edits}}
      <li>
        <a href="/article/{{this.article}}">{{this.creationDate}}</a> by
        <span class="author">{{this.authorName}}</span>
      </li>
      {{/each}}
    </ul>
  </div>
</section>