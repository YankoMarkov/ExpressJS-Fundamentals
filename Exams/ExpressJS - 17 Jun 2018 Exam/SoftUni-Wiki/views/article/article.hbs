<h1>Welcome to SoftUni Wiki!</h1>
<div class="spacer">
  <h2>{{article.title}}
    <div class="controls">
      <a href="/edit/{{article._id}}">[edit]</a>
      <a href="/history/{{article._id}}">[history]</a>
    </div>
  </h2>
  <article>
    {{#each edits}}
    <p>
      {{this.content}}
    </p>
    {{/each}}
  </article>
</div>